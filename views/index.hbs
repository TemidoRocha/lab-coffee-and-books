
    {{#each places}}
      <p>name: "{{ name }}",</p>
      <p>position:</p>
      <p>lat: {{ location.coordinates.[1] }},</p>
      <p>lng: {{ location.coordinates.[0] }}</p>
    {{/each}}


<div id="sample-map" class="map" style="width: 1000px; height: 1000px"></div>

<script>
  const $sampleMap = document.getElementById('sample-map');
  const list = [
    {{#each places}}
      {
        place: "{{ name }}",
        position: {
          lat: {{ location.coordinates.[1] }},
          lng: {{ location.coordinates.[0] }}
        }
      },
    {{/each}}
  ];

  function initMap() {
    const map = new window.google.maps.Map($sampleMap, {
      center: { lat: 38, lng: -9},
      zoom : 9
    });
    
    {{#each places}}
      const marker = new window.google.maps.Marker({
        position: {
          {{ location.coordinates.[1] }},
          {{ location.coordinates.[0] }}
        },
        map
      });
    {{/each}}
  }

</script>

<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBrJ1f_9MB0iFA2zFsHIbIK8sGWU91aQr8&callback=initMap" async defer></script>